//FizzBuzz from eloquent JS
for (var count = 1;count<=100;count++){
  if (count%3 === 0){
    if (count%5 === 0){
     console.log(count + "fizzBuzz");
    }
    else console.log(count + "fizz");
  }
  else if(count%5 === 0){
    if (count%3 === 0){
      console.log(count + "fizzBuzz");
      }
    else console.log(count + "buzz");
  }
  else {
    console.log(count);
    }
  
}

